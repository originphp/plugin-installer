{
    "name": "originphp/plugin-installer",
    "description": "Installer for OriginPHP plugins",
    "type": "composer-installer",
    "license": "MIT",
    "require": {
        "composer-plugin-api": "^1.0"
    },
    "require-dev": {
        "composer/composer": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Origin\\Composer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Origin\\Test\\Composer\\": "tests/"
        }
    },
    "extra": {
        "class": "Origin\\Composer\\PluginInstaller"
    }
}