{
    "name": "originphp/plugin-installer",
    "description": "Installer for OriginPHP plugins",
    "type": "composer-plugin",
    "license": "MIT",
    "require": {
        "php": ">=7.3.0",
        "composer-plugin-api": "^1.0 || ^2.0"
    },
    "require-dev": {
        "composer/composer": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Origin\\Composer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Origin\\Test\\Composer\\": "tests/"
        }
    },
    "extra": {
        "class": "Origin\\Composer\\PluginInstallerPlugin"
    }
}
